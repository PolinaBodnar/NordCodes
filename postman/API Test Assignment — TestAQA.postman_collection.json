							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status 200',()=>pm.response.to.have.status(200));",
									"pm.test('JSON',()=>pm.response.to.be.json);",
									"let json = pm.response.json();",
									"pm.test('Result OK',()=>pm.expect(json.result).to.eql('OK'));",
									"pm.test('Token exists',()=>pm.expect(json.token).to.be.a('string').and.not.empty);",
									"pm.test('Balance is number',()=>pm.expect(json.balance).to.be.a('number'));",
